
# special characters detection

import re
def run(string):
    regex = re.compile('@_!#$%^&*()<>?|?|}{~:]}')
                       
    if regex.search(string)== None :
        print('accepted')
    else:
        print('not accepted')

if __name__ ==" __main__":
    string = 'sejal$is$sejal'
    run(string)                              